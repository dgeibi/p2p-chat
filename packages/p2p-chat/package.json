{
  "name": "p2p-chat",
  "version": "0.7.1",
  "license": "MIT",
  "description": "a p2p chat app",
  "main": "index.js",
  "output": "assets",
  "author": "dgeibi <png.inside@gmail.com> (blog.dgeibi.xyz)",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server",
    "prod": "run-s webpack electron",
    "dist": "run-s webpack builder",
    "lint:js": "eslint .",
    "lint": "run-p lint:*",
    "builder": "build -wl",
    "webpack": "webpack --env.production",
    "electron": "electron .",
    "format": "prettier-eslint --write '**/*.js'"
  },
  "keywords": ["electron-app", "electron", "p2p", "socket", "react", "redux"],
  "dependencies": {
    "chat-core": "^1.2.3",
    "electron-settings": "^3.1.1",
    "fs-extra": "^4.0.1",
    "logger": "github:dgeibi/logger#305933c",
    "utils": "^1.0.0"
  },
  "devDependencies": {
    "antd": "^2.12.6",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-import": "^1.3.1",
    "babel-plugin-react-css-modules": "^3.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.4",
    "dateformat": "^2.0.0",
    "electron": "^1.6.10",
    "electron-builder": "^18.3.5",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-dgeibi": "^3.0.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-no-async-without-await": "^1.2.0",
    "eslint-plugin-react": "^7.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "history": "^4.6.3",
    "html-webpack-plugin": "^2.30.1",
    "ip-regex": "^2.1.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash.isequal": "^4.5.0",
    "lodash.sortby": "^4.7.0",
    "npm-run-all": "^4.0.2",
