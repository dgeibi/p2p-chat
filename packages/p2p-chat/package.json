{
  "name": "p2p-chat",
  "version": "1.4.5",
  "license": "MIT",
  "description": "a P2P LAN chatting and file sharing tool",
  "main": "index.js",
  "output": "assets",
  "author": "dgeibi <png.inside@gmail.com> (blog.dgeibi.xyz)",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server",
    "prod": "run-s clean webpack electron",
    "dist": "run-s clean webpack builder:*",
    "dist:linux": "run-s clean webpack builder:linux",
    "dist:win32": "run-s clean webpack builder:win32",
    "dist:win64": "run-s clean webpack builder:win64",
    "builder:linux": "build -l",
    "builder:win32": "node ./build/build-win-portable.js ia32",
    "builder:win64": "node ./build/build-win-portable.js x64",
    "clean": "rimraf dist assets",
    "webpack": "webpack --env.production",
    "webpack:report": "webpack --env.production --env.report",
    "electron": "electron ."
  },
  "keywords": [
    "electron-app",
    "electron",
    "p2p",
    "socket",
    "react",
    "redux"
  ],
  "dependencies": {
    "electron-settings": "^3.1.1",
    "fs-extra": "^4.0.1",
    "p2p-chat-core": "^1.4.5",
    "p2p-chat-logger": "^1.4.5",
    "p2p-chat-utils": "^1.4.3"
  },
  "devDependencies": {
    "antd": "^2.12.6",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-import": "^1.3.1",
    "babel-plugin-react-css-modules": "^3.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "dateformat": "^2.0.0",
    "electron": "^1.8.0",
    "electron-builder": "^18.8.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "history": "^4.6.3",
    "html-webpack-plugin": "^2.30.1",
    "ip-regex": "^2.1.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash.isequal": "^4.5.0",
    "lodash.sortby": "^4.7.0",
    "lodash.throttle": "^4.1.1",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.2",
    "precss": "^2.0.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0-rc.1",
    "react-dom": "^16.0.0-rc.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.6.1",
    "wtf-webpack-config": "^0.1.3",
    "reselect": "^3.0.1"
  },
  "build": {
    "appId": "xyz.dgeibi.p2pchat",
    "files": [
      "!LICENSE",
      "!webpack-config/**",
      "!webpack.config.js",
      "!package-lock.json",
      "!src/**"
    ],
    "mac": {
      "category": "public.app-category.social-networking"
    },
    "linux": {
      "category": "Network",
      "target": [
        "AppImage"
      ]
    }
  }
}
