{
  "name": "chat-electron",
  "version": "0.6.1",
  "license": "MIT",
  "description": "a p2p chat app",
  "main": "index.js",
  "author": "dgeibi <png.inside@gmail.com> (blog.dgeibi.xyz)",
  "scripts": {
    "start": "webpack-dev-server",
    "start:local": "electron .",
    "builder": "build -wl",
    "dist": "run-s webpack builder",
    "webpack": "webpack --env.production",
    "format": "prettier-eslint --write '**/*.js'",
    "lint:js": "eslint .",
    "lint:css": "stylelint './app/components/**/*.js'",
    "lint": "run-p lint:*",
    "postinstall": "run-p webpack"
  },
  "keywords": [
    "electron-app",
    "electron",
    "p2p",
    "socket"
  ],
  "dependencies": {
    "chat": "^1.2.3",
    "electron-settings": "^3.1.1",
    "fs-extra": "^4.0.1",
    "logger": "github:dgeibi/logger#305933c",
    "utils": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^0.28.4",
    "dateformat": "^2.0.0",
    "electron": "^1.6.10",
    "electron-builder": "^18.3.5",
    "emotion": "^7.0.11",
    "escape-html": "^1.0.3",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-dgeibi": "^3.0.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-no-async-without-await": "^1.2.0",
    "eslint-plugin-react": "^7.2.0",
    "history": "^4.6.3",
    "html-webpack-plugin": "^2.30.1",
    "npm-run-all": "^4.0.2",
    "prettier-eslint-cli": "^4.1.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-styled-components": "^0.2.1",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-node-externals": "^1.6.0",
    "wtf-webpack-config": "^0.1.1"
  },
  "build": {
    "appId": "xyz.dgeibi.p2pchat",
    "files": [
      "!LICENSE",
      "!webpack-config/**",
      "!webpack.config.js",
      "!package-lock.json",
      "!app/**",
      "!src/**"
    ],
    "mac": {
      "category": "public.app-category.social-networking"
    },
    "win": {
      "target": {
        "target": "portable",
        "arch": "ia32"
      }
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 7
          },
          "modules": false,
          "useBuiltIns": true
        }
      ],
      "stage-0",
      "react"
    ],
    "plugins": [
      "emotion/babel"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  }
}
