{
  "name": "p2p-chat",
  "version": "1.3.0",
  "license": "MIT",
  "description": "a p2p chat app",
  "main": "index.js",
  "output": "assets",
  "author": "dgeibi <png.inside@gmail.com> (blog.dgeibi.xyz)",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server",
    "prod": "run-s clean webpack electron",
    "dist": "run-s clean webpack dist:*",
    "dist:win64": "node ./build/build-win-portable.js x64",
    "dist:win32": "node ./build/build-win-portable.js ia32",
    "dist:linux": "build -l",
    "clean": "rimraf dist assets",
    "lint:js": "eslint .",
    "lint": "run-p lint:*",
    "webpack": "webpack --env.production",
    "webpack:report": "webpack --env.production --env.report",
    "electron": "electron .",
    "prettier": "prettier --write '**/*.js' --ignore-path .eslintignore",
    "fix": "eslint --fix .",
    "format": "run-s prettier fix",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "workspaces": [
    "packages/*",
    "."
  ],
  "keywords": [
    "electron-app",
    "electron",
    "p2p",
    "socket",
    "react",
    "redux"
  ],
  "dependencies": {
    "electron-settings": "^3.1.1",
    "fs-extra": "^4.0.1",
    "ip": "^1.1.5",
    "logger": "github:dgeibi/logger#305933c",
    "p2p-chat-core": "^1.3.0",
    "p2p-chat-utils": "^1.3.0"
  },
  "devDependencies": {
    "antd": "^2.12.6",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-import": "^1.3.1",
    "babel-plugin-react-css-modules": "^3.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "dateformat": "^2.0.0",
    "electron": "^1.8.0",
    "electron-builder": "^19.27.7",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-dgeibi": "^3.0.4",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "history": "^4.6.3",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "ip-regex": "^2.1.0",
    "lerna": "^2.1.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lint-staged": "^4.1.3",
    "lodash.isequal": "^4.5.0",
    "lodash.sortby": "^4.7.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.2",
    "precss": "^2.0.0",
    "prettier": "^1.6.1",
    "prop-types": "^15.5.10",
    "react": "^16.0.0-rc.1",
    "react-dom": "^16.0.0-rc.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.6.1",
    "wtf-webpack-config": "^0.1.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "build": {
    "appId": "xyz.dgeibi.p2pchat",
    "files": [
      "!LICENSE",
      "!webpack-config/**",
      "!webpack.config.js",
      "!package-lock.json",
      "!src/**",
      "!packages/**"
    ],
    "mac": {
      "category": "public.app-category.social-networking"
    },
    "linux": {
      "category": "Network",
      "target": [
        "AppImage"
      ]
    }
  }
}
